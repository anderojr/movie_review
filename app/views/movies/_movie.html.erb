<div id="<%= dom_id movie %>">
  <p>
    <strong>Title:</strong>
    <%= movie.title %>
  </p>

  <p>
    <strong>Cover url:</strong>
    <%= movie.cover_url %>
  </p>

  <p>
    <strong>Summary:</strong>
    <%= movie.summary %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= movie.year %>
  </p>

  <p>
    <strong>Duration:</strong>
    <%= movie.duration %>
  </p>

  <p>
    <strong>Suggested date:</strong>
    <%= movie.suggested_date %>
  </p>

  <p>
    <strong>User:</strong>
    <%= movie.user_id %>
  </p>

</div>
